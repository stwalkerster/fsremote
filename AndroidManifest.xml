<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.stwalkerster.android.apps.fsremote" android:installLocation="preferExternal" xmlns:android="http://schemas.android.com/apk/res/android" android:versionName=" rev 20" android:versionCode="20">
	



	
	
	<uses-permission android:name="android.permission.INTERNET"/>


	<uses-feature android:name="FEATURE_TOUCHSCREEN"
		android:required="false"></uses-feature>
	<uses-feature android:name="FEATURE_WIFI"
		android:required="true"></uses-feature>
	<application android:icon="@drawable/icon" android:label="@string/app_name"
		android:enabled="true" android:debuggable="false">


		<activity android:launchMode="standard" android:name="MainMenuActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"></action>
				<category android:name="android.intent.category.LAUNCHER"></category>

			</intent-filter>
		</activity>

		<activity android:name="ConfigMenuActivity">
		</activity>
		<activity android:name="ConsoleActivity"></activity>
	<activity android:name="OverrideButtonsActivity"></activity>
<activity android:name="SequenceActivity"></activity>
</application>

<uses-sdk android:minSdkVersion="3" android:targetSdkVersion="8"></uses-sdk>
</manifest> 